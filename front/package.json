{
    "name": "br",
    "version": "1.0.0",
    "author": "Alexey Chernyshev <er1x4ever@gmail.com>",
    "private": true,
    "scripts": {
        "post-deploy": "yarn build && pm2 reload br",
        "build": "NODE_ENV=production yarn build:webpack && yarn build:lib",
        "build:analyze": "ANALYZE=true NODE_ENV=production webpack",
        "build:lib": "babel -d serverBabel src/lib",
        "build:webpack": "webpack",
        "lint": "eslint ./src/components",
        "start": "NODE_ENV=production SERVER=true node serverBabel/index.js",
        "start:dev": "rm -rf ./dist/node/*.hot-update.* && rm -rf ./dist/public/static/*.hot-update.* && SERVER=true nodemon --exec babel-node src/lib/index.js -w ./src/lib",
        "start:dev:ssr": "SSR=true yarn start:dev",
        "stylelint": "stylelint ./src/**/*.css",
        "env": "sync-dotenv"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "firefox esr",
        "not dead",
        "not ie_mob 11",
        "not op_mob > 0",
        "not op_mini all",
        "not and_qq > 0"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ],
        "*.{css}": [
            "stylelint --fix",
            "prettier --write",
            "git add"
        ],
        "*.{json}": [
            "prettier --write",
            "git add"
        ]
    },
