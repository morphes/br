mutation ($input: AddBasketInput!) {
  addBasket(input: $input) {
    products {
      qty
      price
      name
      item{
        id
      }
      url
      lenses {
        name
        price
        options {
          name
          value
        }
        recipes {
          sides {
            left {
              name
              value
            }
            right {
              name
              value
            }
          }
          extraData {
            name
            value
          }
        }
      }
    }
  }
}

{
  "input": {
